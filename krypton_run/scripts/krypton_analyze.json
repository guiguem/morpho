{
  "stan": {

  	 "name": "krypton_analysis",

	 "model":	{
	 	"file": "./krypton_run/models/krypton_analysis.stan",
		"cache": "./krypton_run/cache"
		},

	 "data": {
	 	"type": "data",
		"files": [
		    {
			"name": "./krypton_run/data/krypton_run.header.data", 
			"format":"R"
		    },
	 	    {
			"name" : "./krypton_run/data/TestTracks_Krdatacold_520MHz_trapon2A.root", 
			"format" : "root",
			"tree" : "procTracks",
			"cut": "MinFrequency > 0 && ! IsCut",
			"branches" : [
				   {"name" : "MinFrequency", "stan_alias" : "freq_data"},
				   {"name" : "Slope", "stan_alias" : "dfdt_data"}
				   ]
		     }
		 ],
		"parameters": [
		      {
		        "usePower" : 0,
			"fclock" : 24.7200e9
		      }
		    ]
	},

	"run": {
	       "algorithim": "NUTS",
	       "iter": 5000,
	       "chain": 4,
	       "pars_off": ["TotalField", "TrappingField", "MainField", "SourceMean", "SourceWidth"]
	       },

	"plot_off": [
		{"variable": "TotalField"},
		{"variable": "TrappingField"},
		{"variable": "MainField"},
		{"variable": "SourceMean"},
		{"variable": "SourceWidth"}
		],

	"output": 
		  {
		   "name" : "./krypton_run/results/test_analysis_Krdatacold_520MHz_trapon2A.root",
		   "format" : "root",
		   "tree" : "stanAnalysis",
		   "branches" : [
		   	   {"variable" : "TotalField", "root_alias" : "TotalField"},
		   	   {"variable" : "TrappingField", "root_alias" : "TrappingField"},
		   	   {"variable" : "MainField", "root_alias" : "MainField"},
		   	   {"variable" : "gPower", "root_alias" : "gPower"},
		   	   {"variable" : "df", "root_alias" : "df"},
		   	   {"variable" : "muB", "root_alias" : "muB"},
		   	   {"variable" : "uCauchy", "root_alias" : "uCauchy"},
		   	   {"variable" : "SourceMean", "root_alias" : "SourceMean"},
		   	   {"variable" : "SourceWidth", "root_alias" : "SourceWidth"},
		   	   {"variable" : "SourceFrequency", "root_alias" : "SourceFrequency"},
		   	   {"variable" : "SourcePower", "root_alias" : "SourcePower"},
		   	   {"variable" : "SourceSlope", "root_alias" : "SourceSlope"},
		   	   {"variable" : "avg_KE", "root_alias" : "avg_KE"},
		   	   {"variable" : "avg_stheta", "root_alias" : "avg_stheta"},
		   	   {"variable" : "lp__", "root_alias" : "LogLikelihood"}
		    	   ]
		   }	   
   }

}
