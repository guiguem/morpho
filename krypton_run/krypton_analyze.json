{
  "stan": {

  	 "name": "krypton_analysis",

	 "model":	{
	 	"file": "./krypton_analysis.stan",
		"cache": "./cache"
		},

	 "data": {
	 	"type": "data",
		"files": [
		    {
			"name": "./data/krypton_run.header.data", 
			"format":"R"
		    },
	 	    {
			"name" : "./data/TestTracks_Krdatacold_520MHz_trapon2A.root", 
			"format" : "root",
			"tree" : "procTracks",
			"cut_off": "Intercept > 0 && Slope > 0",
			"branches" : [
				   {"name" : "Intercept", "stan_alias" : "freq_data"},
				   {"name" : "Slope", "stan_alias" : "dfdt_data"}
				   ]
		     }
		 ],
		"parameters": [
		      {
			"minFreq" : 24.2e9,
			"maxFreq" : 26.0e9,
			"fclock" : 24.750e9
		      }
		    ]
	},

	"run": {
	       "algorithim": "NUTS",
	       "iter": 2500,
	       "chain": 1},

	"plot": [
		{"variable": "TotalField"},
		{"variable": "TrappingField"},
		{"variable": "MainField"},
		{"variable": "stheta"},
		{"variable": "SourceMean"},
		{"variable": "SourceWidth"},
		{"variable": "KE"}		
		],

	"sample": "./results/test_generator.out"

   }

}
