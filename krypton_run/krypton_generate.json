{
  "stan": {

  	 "name": "krypton_generator",

	 "model":	{
	 	"file": "./krypton_generator.stan",
		"cache": "./cache"
		},

	 "data": {
	 	"type": "mc",
		"files": [
		    {
			"name": "./data/krypton_run.header.data", 
			"format":"R"
		    },
		    {
			"name": "./data/krypton_monte_carlo.data", 
			"format":"R"
		    }
		 ]
	},

	"run": {
	       "algorithim": "NUTS",
	       "iter": 5000,
	       "chain": 12},

	"plot": [
		{"variable": "TotalField"},
		{"variable": "TrappingField"},
		{"variable": "MainField"},
		{"variable": "KE"},
		{"variable": "frequency"},
		{"variable": "stheta"}
		],

	"sample": "./results/test_generator.out",

	"output": 
		  {
		   "name" : "./test_output.root",
		   "format" : "root",
		   "tree" : "procTracks",
		   "branches" : [
		   	   {"variable" : "stheta", "root_alias" : "stheta"},
		   	   {"variable" : "KE", "root_alias" : "KE"},
		   	   {"variable" : "TotalField", "root_alias" : "TotalField"},
		   	   {"variable" : "frequency", "root_alias" : "freq_data"},
		   	   {"variable" : "dfdt", "root_alias" : "dfdt_data"},
		   	   {"variable" : "power", "root_alias" : "power_data"}
		    	   ]
		   }	   

   }

}
