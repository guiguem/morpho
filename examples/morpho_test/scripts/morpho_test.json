{
   "morpho":
   {
     "do_stan": True,
     "do_postprocessing": False,
     "do_plots": False
   },
    "stan": {

        "name": "morpho_test",

        "model":   {
            "file": "./morpho_test/models/morpho_test.stan",
            "function_file": [
                              ],
            "cache": "./morpho_test/cache"
        },

        "run": {
            "algorithm": "NUTS",
            "iter": 2000,
            "warmup" : 200,
            "chain": 4,
            "control": {
                "adapt_kappa": 0.75,
                "adapt_engaged": false
              }
            },


        "sample": "./morpho_test/results/morpho_test.out",

        "output":
        {
            "name" : "./morpho_test/results/morpho_test",
            "format": "hdf5",
            "group" : "morpho_test",
            "branches": [
                 {"variable":"theta", "root_alias":"theta"},
                 {"variable":"theta2", "root_alias":"theta2","ndim":2}
               ],
            "inc_warmup": true
         }
    },

"postprocessing":
  {
    "which_pp":[
      {
      }
   ]
   },

"plot":
    {
      "which_plot":[
        {
        }
        ]
    }
}
